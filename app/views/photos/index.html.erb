<% unless request.xhr? %>
  <%= render partial: 'shared/welcome_message', locals: { message: '- Photos -' } %>
<% end %>

<!-- Start portfolio -->  
<section class="portfolio">
  <div class="container">
    <div class="row">                                                                                       
      <div class="span12">                        
        <ul class="thumbnails">
          <% (1..10).each do | photo_index | %>
            <li class="span2">
              <div class="thumbnail">
                <div class="portfolio-item">
                  <img src="/assets/property/thumbs/<%= "%02d" % photo_index %>.jpg" alt="">
                  <div class="folio-body">
                    <div class="folio-info">
                    </div>
                    <div class="folio-zoom">
                      <a href="/assets/property/<%= "%02d" % photo_index %>.jpg" rel="prettyPhoto[pp_gal]"><i class="icon-plus"></i> Zoom</a>
                    </div>
                  </div>
                </div>
              </div>
            </li>
          <% end %>
        </ul>
        <hr>
      </div>
    </div>
  </div>
</section> 
<!-- End portfolio -->   

